<h2>Main Content</h2>
<div class="row">
    <% for(let i = 0; i < products.length; i++){%>
        <div class="col-4">
            <div class="card">
                <div class="card-header">
                    <h3><%= products[i].title; %></h3>
                </div>
                <div class="card-body">
                    <img src="./images/mobile1.jpg" class="container" />
                </div>
                <div class="card-footer">
                    <span>Price : $<%= products[i].price; %>/-</span>
                </div>
            </div>
        </div>
    <%} %>
</div>
    
    